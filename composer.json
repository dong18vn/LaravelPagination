{
    "name": "nid/laravel-pretty-pagination",
    "description": "A package for pretty pagination in Laravel",
    "type": "library",
    "require": {
        "php": "^8.2",
        "illuminate/container": "^6.0|^7.0|^8.0|^9.0|^10.0|^11.0",
        "illuminate/http": "^6.0|^7.0|^8.0|^9.0|^10.0|^11.0",
        "illuminate/pagination": "^6.0|^7.0|^8.0|^9.0|^10.0|^11.0",
        "illuminate/routing": "^6.0|^7.0|^8.0|^9.0|^10.0|^11.0",
        "illuminate/support": "^6.0|^7.0|^8.0|^9.0|^10.0|^11.0"
    },
    "autoload": {
        "psr-4": {
            "Nid\\LaravelPrettyPagination\\": "src/"
        }
    },
    "extra": {
        "laravel": {
            "providers": [
                "Nid\\LaravelPrettyPagination\\Providers\\PrettyPaginationProvider"
            ]
        }
    },
    "minimum-stability": "dev",
    "prefer-stable": true
}
